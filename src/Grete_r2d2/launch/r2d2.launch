<launch>
<param name="robot_description" command ="xacro --inorder '$(find Grete_r2d2)/urdf/r2d2.urdf.xacro'"/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type ="robot_state_publisher"/>
<node name="joint_state_publisher" pkg="joint_state_publisher" type ="joint_state_publisher"/>
<remap from="cmd_vel" to="/turtle1/cmd_vel"/>
<node name="r2d2_navigator" pkg="r2d2_navigator" type ="fake_r2d2_controller"/>
<node name="rviz" pkg ="rviz" type ="rviz" args="-d $(find Grete_r2d2)/config/r2d2.rviz"/>
</launch>

